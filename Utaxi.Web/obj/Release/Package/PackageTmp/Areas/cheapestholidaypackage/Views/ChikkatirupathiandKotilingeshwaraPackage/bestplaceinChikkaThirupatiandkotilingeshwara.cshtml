<div class="modal fade" id="myOutstationModal">
    <div class="modal-dialog modal-dialog-centered" style="max-width: 535px !important;">
        <div class="modal-content">

            <!-- Modal Header -->
            <div class="modal-header">
                <h4 class="modal-title">Outstation Booking</h4>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>

            <!-- Modal body -->
            <div class="modal-body">
                <img src="~/Images/please-reach-out-to-our-customer-care-for-the-holiday-package-booking.png" alt="Book taxi fro one day holiday package" />
            </div>

            <!-- Modal footer -->
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
            </div>

        </div>
    </div>
</div>
